import java.util.Scanner;
public class ages {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter Age: ");
        int age=sc.nextInt();
        if (age<0) System.out.println("INVALID Age entered!");
        else if (age<=12) System.out.println("You are a child");
        else if (age<=19) System.out.println("You are a teenager");
        else if (age<=60) System.out.println("You are an adult");
        else System.out.println("You are a senior");
    }
}
