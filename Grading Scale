import java.util.Scanner;
public class grades {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter your score(0-100) to check grades: ");
        int score=sc.nextInt();
        if (score <0 || score > 100) System.out.println("INVALID SCORE!");
        else if (score > 85) System.out.println("Your Grade is: A");
        else if (score > 72) System.out.println("Your Grade is: B");
        else if (score > 55) System.out.println("Your Grade is: C");
        else if (score > 33) System.out.println("Your Grade is: D");
       else System.out.println("Your Grade is: F");
    }
}
